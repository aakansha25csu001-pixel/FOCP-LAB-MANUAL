/*#include<iostream>
using namespace std;
int number(int);
int n,result;
int main (){
    cout<<"enter a number:";
    cin>>n;
    result=number(n);
    if(result==1){
        cout<<"PRIME";
    }
    else
    cout<<"NOT PRIME";
    return 0;
}

int number(int n)
{
    int count=0;
   for(int i=1;i<=n;i++){
    if (n%i==0){
        count++;
    }
}
    if (count==2)
    
        return 1;
    
    else
    return 0;  
}*/
