/*#include<iostream>
using namespace std;
float areacircle(int a);
int arearectangle(int l,int b);
int areatriangle(int b,int h);
int main(){ 
    cout<<"1.AREA OF CIRCLE\n";
    cout<<"1.AREA OF RECTANGLE\n";
    cout<<"1.AREA OF TRIANGLE\n";
    int l,b,h,a;
    int choice;

    cout<<"enter choice:";
    cin>>choice;
    
    if (choice==1){
        cout<<"enter radius:";
        cin>>a;
        cout<<"the area of circle:"<<areacircle(a);
    }
    else if (choice==2){ 
        cout<<"enter length n breadth:";
        cin>>l>>b;
        cout<<"the area of rectangle:"<<arearectangle(l,b);
       
    }
    else if (choice==3){
        cout<<"enter basee n height:";
        cin>>b>>h;
        cout<<"the area of triangle"<<areatriangle(b,h);
    }
    else 
    cout<<"INVALID";
    return 0;
 } 
float areacircle(int a){
   float areacircle=3.14*a*a;
     return areacircle;
    }
int arearectangle(int l,int b){
 int arearectangle=l*b;
 return arearectangle;
}
int areatriangle(int b,int h){
    int areatriangle=0.5*b*h;
    return (areatriangle);
}*/
